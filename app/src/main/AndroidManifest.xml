<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="wen.example.constraintlayout">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClExample"
        tools:targetApi="31">
        <activity
            android:name=".guideline.TestGuideLineActivity"
            android:exported="false" />
        <activity
            android:name=".barrier.TestBarrierActivity"
            android:exported="false" />
        <activity
            android:name=".baseline.TestBaseLineActivity"
            android:exported="false" />
        <activity
            android:name="wen.example.constraintlayout.chain.TestChainActivity"
            android:exported="false" />
        <activity
            android:name="wen.example.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>